n        <input\r\n          v-model=\"searchQuery\"\r\n          type=\"text\"\r\n          placeholder=\"–ü–æ–∏—Å–∫ –ø–æ –§–ò–û\"\r\n          class=\"search-input\"\r\n        />\r\n      </div>\r\n      <button\r\n        @click=\"saveAll\"\r\n        class=\"save-button\"\r\n        :disabled=\"!hasChanges || saving\"\r\n      >\r\n        {{ saving ? \"–°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ...\" : \"–°–æ—Ö—Ä–∞–Ω–∏—Ç—å –≤—Å–µ –∏–∑–º–µ–Ω–µ–Ω–∏—è\" }}\r\n      </button>\r\n    </div>\r\n\r\n    <div v-if=\"loading\" class=\"loading-state\">\r\n      <div class=\"spinner\"></div>\r\n      <span>–ó–∞–≥—Ä—É–∑–∫–∞ –¥–∞–Ω–Ω—ã—Ö...</span>\r\n    </div>\r\n\r\n    <div v-if=\"error\" class=\"error-state\">\r\n      <span class=\"error-icon\">!</span>\r\n      <span>{{ error }}</span>\r\n    </div>\r\n\r\n    <div class=\"user-list\" v-if=\"!loading\">\r\n      <div class=\"user-item\" v-for=\"user in filteredUsers\" :key=\"user.id\">\r\n        <div class=\"user-info\">\r\n          <span class=\"user-name\">{{ user.full_name }}</span>\r\n          <span class=\"current-key\" v-if=\"user.key\">\r\n            –¢–µ–∫—É—â–∏–π –∫–ª—é—á: <strong>{{ user.key.uid }}</strong>\r\n          </span>\r\n          <span class=\"no-key\" v-else>–ö–ª—é—á –Ω–µ –ø—Ä–∏–≤—è–∑–∞–Ω</span>\r\n        </div>\r\n\r\n        <div class=\"key-selection\">\r\n          <select\r\n            v-model=\"user.key_id\"\r\n            class=\"key-combobox\"\r\n            @change=\"markAsChanged(user.id)\"\r\n          >\r\n            <option :value=\"null\">-- –ö–ª—é—á –Ω–µ –≤—ã–±—Ä–∞–Ω --</option>\r\n            <option\r\n              v-for=\"key in availableKeys\"\r\n              :value=\"key.id\"\r\n              :key=\"key.id\"\r\n              :disabled=\"isKeyAssigned(key.id, user.id)\"\r\n            >\r\n              {{ key.uid }}\r\n            </option>\r\n          </select>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-if=\"!loading && filteredUsers.length === 0\" class=\"empty-state\">\r\n      <span class=\"empty-icon\">üë§</span>\r\n      <span>–ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–∏ –Ω–µ –Ω–∞–π–¥–µ–Ω—ã</span>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport EspData from \"../services/data_import\";\r\n\r\nexport default {\r\n  name: \"KeyManagement\",\r\n  data() {\r\n    return {\r\n      users: [], // –°–æ–¥–µ—Ä–∂–∏—Ç full_name, key, key_id\r\n      keys: [], // –°–æ–¥–µ—Ä–∂–∏—Ç —Ç–æ–ª—å–∫–æ id –∏ uid –∫–ª—é—á–µ–π\r\n      loading: false,\r\n      saving: false,\r\n      error: null,\r\n      searchQuery: \"\",\r\n      changedUsers: new Set(),\r\n    };\r\n  },\r\n  computed: {\r\n    filteredUsers() {\r\n      const query = this.searchQuery.toLowerCase();\r\n      return this.users.filter((user) =>\r\n        user.full_name.toLowerCase().includes(query)\r\n      );\r\n    },\r\n    availableKeys() {\r\n      return this.keys;\r\n    },\r\n    hasChanges() {\r\n      return this.changedUsers.size > 0;\r\n    },\r\n  },\r\n  methods: {\r\n    async fetchData() {\r\n      this.loading = true;\r\n      this.error = null;\r\n      try {\r\n        const [keysResponse, peopleResponse] = await Promise.all([\r\n          EspData.getKey(),\r\n          EspData.getPeople(),\r\n        ]);\r\n\r\n        this.keys = keysResponse.data;\r\n        this.users = peopleResponse.data;\r\n\r\n        // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∏—Ä—É–µ–º –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏–µ –∏–∑–º–µ–Ω–µ–Ω–∏–π\r\n        this.changedUsers = new Set();\r\n      } catch (err) {\r\n        this.error = err.response?.data?.message || \"–û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ –¥–∞–Ω–Ω—ã—Ö\";\r\n        console.error(\"–û—à–∏–±–∫–∞ –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ:\", err);\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n    isKeyAssigned(keyId, currentUserId) {\r\n      if (!keyId) return false;\r\n      return this.users.some(\r\n        (user) => user.key_id === keyId && user.id !== currentUserId\r\n      );\r\n    },\r\n    markAsChanged(userId) {\r\n      this.changedUsers.add(userId);\r\n    },\r\n    async saveAll() {\r\n      if (!this.hasChanges) return;\r\n\r\n      this.saving = true;\r\n      this.error = null;\r\n\r\n      try {\r\n        // –§–∏–ª—å—Ç—Ä—É–µ–º —Ç–æ–ª—å–∫–æ –∏–∑–º–µ–Ω–µ–Ω–Ω—ã—Ö –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π\r\n        const usersToUpdate = this.users.filter((user) =>\r\n          this.changedUsers.has(user.id)\r\n        );\r\n\r\n        // –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –∏–∑–º–µ–Ω–µ–Ω–∏—è\r\n        for (const user of usersToUpdate) {\r\n          await EspData.updateKey(user.id, { key_id: user.key_id });\r\n        }\r\n\r\n        // –ü–æ—Å–ª–µ —É—Å–ø–µ—à–Ω–æ–≥–æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è —Å–±—Ä–∞—Å—ã–≤–∞–µ–º –∏–∑–º–µ–Ω–µ–Ω–∏—è\r\n        this.changedUsers = new Set();\r\n        this.$notify({\r\n          title: \"–£—Å–ø–µ—Ö\",\r\n          message: \"–ò–∑–º–µ–Ω–µ–Ω–∏—è —É—Å–ø–µ—à–Ω–æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω—ã\",\r\n          type: \"success\",\r\n        });\r\n      } catch (err) {\r\n        this.error = err.response?.data?.message || \"–û—à–∏–±–∫–∞ –ø—Ä–∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–∏\";\r\n        this.$notify({\r\n          title: \"–û—à–∏–±–∫–∞\",\r\n          message: this.error,\r\n          type: \"error\",\r\n        });\r\n      } finally {\r\n        this.saving = false;\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchData();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* –°—Ç–∏–ª–∏ –æ—Å—Ç–∞—é—Ç—Å—è —Ç–∞–∫–∏–º–∏ –∂–µ, –∫–∞–∫ –≤ –≤–∞—à–µ–º –ø—Ä–µ–¥—ã–¥—É—â–µ–º –∫–æ–¥–µ */\r\n.key-management {\r\n  max-width: 900px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  font-family: \"Arial\", sans-serif;\r\n}\r\n\r\n.page-title {\r\n  text-align: center;\r\n  margin-bottom: 25px;\r\n  color: #2c3e50;\r\n  font-size: 24px;\r\n}\r\n\r\n.controls {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n  gap: 15px;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.search-box {\r\n  flex-grow: 1;\r\n  min-width: 250px;\r\n}\r\n\r\n.search-input {\r\n  width: 100%;\r\n  padd