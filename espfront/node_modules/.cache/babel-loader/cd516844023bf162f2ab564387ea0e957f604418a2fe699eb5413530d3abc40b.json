nt-weight: 600;\r\n}\r\n\r\n.reader-badge {\r\n  display: inline-block;\r\n  padding: 0.4rem 0.8rem;\r\n  background: #f0f4ff;\r\n  color: #4361ee;\r\n  border-radius: 12px;\r\n  font-size: 0.85rem;\r\n  font-weight: 600;\r\n}\r\n\r\n.uid-cell {\r\n  font-family: \"Fira Code\", monospace;\r\n  color: #555;\r\n  letter-spacing: 0.5px;\r\n}\r\n\r\n.status-badge {\r\n  display: inline-block;\r\n  padding: 0.5rem 1rem;\r\n  border-radius: 12px;\r\n  font-size: 0.85rem;\r\n  font-weight: 600;\r\n}\r\n\r\n.status-success {\r\n  background: #e6f7ee;\r\n  color: #00a854;\r\n}\r\n\r\n.status-error {\r\n  background: #ffebee;\r\n  color: #f44336;\r\n}\r\n\r\n.date-cell {\r\n  color: #666;\r\n  font-size: 0.9rem;\r\n}\r\n\r\n.last-row {\r\n  border-bottom: none;\r\n}\r\n\r\n/* –°–æ—Å—Ç–æ—è–Ω–∏—è */\r\n.loading-state,\r\n.error-state,\r\n.empty-state {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  gap: 0.8rem;\r\n  padding: 1.5rem;\r\n  margin-top: 2rem;\r\n  border-radius: 12px;\r\n  font-size: 1rem;\r\n  font-weight: 500;\r\n}\r\n\r\n.loading-state {\r\n  background: #f5f9ff;\r\n  color: #4361ee;\r\n}\r\n\r\n.error-state {\r\n  background: #fff5f5;\r\n  color: #f44336;\r\n}\r\n\r\n.empty-state {\r\n  background: #f9f9f9;\r\n  color: #666;\r\n}\r\n\r\n.spinner {\r\n  width: 1.25rem;\r\n  height: 1.25rem;\r\n  border: 3px solid rgba(67, 97, 238, 0.2);\r\n  border-top-color: #4361ee;\r\n  border-radius: 50%;\r\n  animation: spin 1s linear infinite;\r\n}\r\n\r\n.error-icon,\r\n.empty-icon {\r\n  font-size: 1.2rem;\r\n}\r\n\r\n@keyframes spin {\r\n  to {\r\n    transform: rotate(360deg);\r\n  }\r\n}\r\n\r\n/* –ê–Ω–∏–º–∞—Ü–∏—è –ø–æ—è–≤–ª–µ–Ω–∏—è —Å—Ç—Ä–æ–∫ */\r\n.table-row {\r\n  animation: fadeIn 0.4s ease forwards;\r\n  opacity: 0;\r\n}\r\n\r\n@keyframes fadeIn {\r\n  to {\r\n    opacity: 1;\r\n  }\r\n}\r\n\r\n/* –ê–¥–∞–ø—Ç–∏–≤–Ω–æ—Å—Ç—å */\r\n@media (max-width: 768px) {\r\n  .access-logs {\r\n    padding: 1rem;\r\n  }\r\n\r\n  .header-cell,\r\n